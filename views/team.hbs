<h3>Our Team</h3>
<hr/>
<div class="container">
<div class="row">
  {{#each details}}
    <div class="col m4">
      <div class="card">
        <div class="card-image">
          <img id="crop-image" src="images/{{img}}.jpg">
        </div>
        <div class="card-content">
          <h5>{{name}}</h5>
          <h6 class="shade">Designation: {{designation}}</h6>
        </div>
        <div class="card-action">
          <h6>Email: {{email}}</h6>
          <h6>Contact No.: {{phone}}</h6>
        </div>
      </div>
    </div>
     {{/each}}
    </div>
   </div> 
